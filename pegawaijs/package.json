{
    "name": "@coresimrs/pegawaijs",
    "version": "0.1.0",
    "private": true,
    "main": "dist/App.js",
    "unpkg": "dist/App.js",
    "jsdelivr": "dist/App.js",
    "files": [
        "src",
        "dist/*.js",
        "dist/*.css",
        "dist/locales/*.json",
        "types/*.d.ts"
    ],
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.5",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.5.0",
        "backendjs": "file:../backendjs",
        "bootstrap": "^5.3.2",
        "cipher-base": "^1.0.4",
        "crypto-js": "^4.1.1",
        "cryptojs": "^2.5.3",
        "daftarmandiri": "file:../daftarmandiri",
        "dotenv": "^16.4.5",
        "formik": "^2.4.5",
        "frontendjs": "file:../frontendjs",
        "prettier": "^3.0.3",
        "rc-progress": "^3.5.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-flatpickr": "^3.10.13",
        "react-loader-spinner": "^5.4.5",
        "react-redux": "^8.1.2",
        "react-responsive-carousel": "^3.2.23",
        "react-router": "^6.23.1",
        "react-router-dom": "^6.23.1",
        "react-scripts": "5.0.1",
        "react-select": "^5.7.5",
        "react-toastify": "^9.1.3",
        "react-webcam": "~7.2.0",
        "reactstrap": "^9.2.0",
        "redux": "^4.2.1",
        "redux-saga": "^1.2.3",
        "uuid": "^9.0.1",
        "web-vitals": "^2.1.4",
        "webpack-cli": "~5.1.4",
        "yup": "^1.3.0"
    },
    "scripts": {
        "start": "webpack serve --open --hot --mode development",
        "deploy": "nodemon --watch ../sharedjs --exec \"react-scripts start\"",
        "build-react": "react-scripts build --max_old_space_size=8000 build",
        "build": "",
        "build-webpack": "webpack --mode production",
        "build-babel": "babel src --out-dir dist --copy-files",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\""
    },
    "babelOptions": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "react-app"
        ],
        "parser": "@babel/eslint-parser",
        "parserOptions": {
            "requireConfigFile": false,
            "ecmaVersion": 2020,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "env": {
            "browser": true,
            "es6": true
        },
        "globals": {
            "$": true
        },
        "settings": {
            "react": {
                "version": "^17.0.1"
            }
        },
        "rules": {
            "semi": 0,
            "react/no-unescaped-entities": 0,
            "react/react-in-jsx-scope": "off",
            "react/prop-types": "off",
            "no-unused-vars": "off",
            "no-extra-boolean-cast": 0
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "moment": "2.29.1",
        "autoprefixer": "10.4.5"
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    },
    "devDependencies": {
        "@types/react-flatpickr": "^3.8.8",
        "@types/styled-components": "^5.1.26",
        "assert": "^2.1.0",
        "babel-plugin-prismjs": "^2.1.0",
        "buffer": "^6.0.3",
        "process": "^0.11.10",
        "react-router-dom": "^6.16.0",
        "sass": "^1.67.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "url": "^0.11.3"
    }
}
